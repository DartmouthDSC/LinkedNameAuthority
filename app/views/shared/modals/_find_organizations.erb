<div class="modal fade findOrgsModal formModal" id="findOrgsModal" tabindex="-1" role="dialog" aria-labelledby="findOrgsModal" title="Find Organizations">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-body">
        <form data-lna-query="findOrgs" accept-charset="UTF-8" data-no-ajax="true" class="formSingleColumn">
          <label>Name</label><input name="skos:prefLabel"></input>
          <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
          <button type="submit" title="submit">Submit</button>
        </form>
      </div>
    </div>
  </div>
</div>

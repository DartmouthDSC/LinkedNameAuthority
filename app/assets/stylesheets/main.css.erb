.breadcrumbs{
    max-width: 55em;
    padding: 0 1em;
    display: -webkit-flex;
    display: flex;
    align-items: center;
    -webkit-flex-wrap: wrap;
    flex-wrap: wrap;
}
.breadcrumbs a{
    display: inline-block;
    text-decoration: none;
    /*color: #F2F4F3;*/
    color: white;
    font-weight: 600;
    display: -webkit-flex;
    display: flex;
    -webkit-align-items: center;
    align-items: center;
    margin-bottom: .2em;
}
.breadcrumbs a.crumbHere{
    font-size: 1.4em;
}
.breadcrumbs a:hover, .breadcrumbs a:focus{
    text-shadow: 0 .1em rgba(0,0,0,.6);
}
.breadcrumbs a::before{
    display: inline-block;
    background-color: #95AFA2;
    margin-right: -1em;
    width: 2em;
    height: 2em;
    border-radius: 1em;
    content: " ";
    -moz-border-radius: 1em;
    -webkit-border-radius: 1em;
    z-index: 0;
}
.breadcrumbs .crumbContinue::after{
    display: inline-block;
    width: 0;
    height: 0;
    border-top: 1em solid transparent;
    border-left: 2em solid #95AFA2;
    border-bottom: 1em solid transparent;
    content: " ";
    z-index: 0;
}
.breadcrumbs .crumbStop::after{
    display: inline-block;
    background-color: #95AFA2;
    margin-left: -1em;
    width: 2em;
    height: 2em;
    border-top: 0;
    border-left: 0;
    border-bottom: 0;
    border-radius: 1em;
    content: " ";
    -moz-border-radius: 1em;
    -webkit-border-radius: 1em;
    z-index: 0;
}
.breadcrumbs a span{
    font-size: 1em;
    height: 2em;
    line-height: 2em;
    background-color: #95AFA2;
    display: inline-block;
    vertical-align: top;
    position:relative;
    z-index: 1;
}
.breadcrumbs .add{
    display: inline-block;
    background-color: #95AFA2;
    width: 2em;
    height: 2em;
    border-radius: 1em;
    -moz-border-radius: 1em;
    -webkit-border-radius: 1em;
}
.breadcrumbs .add span{
    background-color: transparent;
    font-size: 2em;
    line-height: .95em;
    margin-left: -.27em;
}

